{
  "name": "diapio-holder",
  "version": "1.3.1",
  "type": "module",
  "scripts": {
    "dev:fe": "cd src/frontend && bun dev",
    "dev:be": "cd src/backend && bun run dev",
    "i:fe": "cd src/frontend && bun install",
    "i:be": "cd src/backend && bun install",
    "i:package": "cd src/package && bun install",
    "dev": "concurrently \"bun run dev:fe\" \"bun run dev:be\"",
    "build:fe": "cd src/frontend && bun run build",
    "build:be": "cd src/backend && bun run build",
    "build:package": "cd src/package && bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/zerK4/diapio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerK4/diapio.git"
  },
  "bugs": "https://github.com/zerK4/diapio/issues",
  "author": "Sebastian Pavel <sebastian.v.pavel@gmail.com  >",
  "dependencies": {
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "json-to-ts": "^2.1.0"
  }
}